  "scripts": {
    "dev:kdx": "turbo watch dev studio -F @kdx/app... @kdx/db-dev#dev",
    "dev:cash": "turbo watch dev studio -F @cash/app... @cash/db-dev#dev",
    "dev:email": "turbo dev -F @kdx/react-email...",
    "push:kdx": "turbo @kdx/db-dev#dev @kdx/db#push",
    "push:cash": "turbo @cash/db-dev#dev @cash/db#push",
    "push:cash:preview": "bun --filter @cash/db push:preview",
    "push:cash:production": "bun --filter @cash/db push:production",
    "seed:kdx": "turbo @kdx/db-dev#dev @kdx/db#seed",
    "seed:cash": "turbo @cash/db-dev#dev @cash/db#seed",
    "seed:cash:preview": "bun --filter @cash/db seed:preview",
    "check": "biome check .",
    "check:write": "biome check --write .",
    "check:unsafe": "biome check --write --unsafe .",
    "start:trpc-cli": "bun --filter trpc-cli start",
    "dev:trpc-cli": "bun --filter trpc-cli dev",
    "vercel-link": "vercel switch kodix && vercel link --repo --yes && bun --filter='*' pull-vercel"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@turbo/gen": "catalog:turbo",
    "turbo": "catalog:turbo",
