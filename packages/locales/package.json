{
  "name": "@kdx/locales",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./server": "./src/server.ts",
    "./client": "./src/client.ts",
    "./provider": "./src/provider.tsx",
    "./lang": "./src/lang.ts",
    "./hooks": "./src/hooks/index.ts",
    "./server-hooks": "./src/hooks/server-hooks.ts"
  },
  "scripts": {
    "dev": "nodemon --watch ./src/lang --ext json --exec \"pnpm run generate\"",
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check . '!src/generated/**' --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "generate": "node --import tsx/esm ./src/generate.ts"
  },
  "dependencies": {
    "@kdx/shared": "workspace:*",
    "next-international": "^1.2.4",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@kdx/eslint-config": "workspace:*",
    "@kdx/prettier-config": "workspace:*",
    "@kdx/tsconfig": "workspace:*",
    "@types/react": "^18.2.71",
    "eslint": "^9.0.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "tsx": "4.7.2",
    "typescript": "^5.4.5"
  },
  "prettier": "@kdx/prettier-config"
}
